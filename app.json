{
  "expo": {
    "name": "Guru",
    "slug": "guru",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "guru",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#373737",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.READ_CONTACTS",
        "android.permission.WRITE_CONTACTS"
      ],
      "googleServicesFile": "./google-services.json",
      "package": "com.phantom.guru"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-3603875446667492~8760578808",
          "iosAppId": "ca-app-pub-3603875446667492~5124586123",
          "userTrackingUsageDescription": "This identifier will be used to deliver personalized ads to you."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/notifications.png",
          "color": "#E6F4FE",
          "defaultChannel": "default",
          "sounds": [
            "./assets/sounds/alert_notification.wav"
          ],
          "enableBackgroundRemoteNotifications": true
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#373737",
          "dark": {
            "backgroundColor": "#373737"
          }
        }
      ],
      [
        "expo-contacts",
        {
          "contactsPermission": "Allow Guru App to access your contacts."
        }
      ],
      "expo-font",
      "expo-web-browser"
    ],
    "extra": {
      "eas": {
        "projectId": "22260cf2-a686-42bd-9c40-8aa6580ca694"
      },
      "router": {}
    },
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/affb8479-1bf1-4481-b621-b87fa89236a0"
    },
    "owner": "dhannnyphantom"
  }
}
